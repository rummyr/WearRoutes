<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
<android.support.wearable.view.ActionPage
    android:id="@+id/action_stop"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:text="End"
    android:saveEnabled="false"
    android:clickable="true"
    android:src="@drawable/materialdesignicons_clock_end_red"
    style="@style/Widget.ActionPage"
    android:visibility="visible"
    />
<android.support.wearable.view.ActionPage
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/action_start"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="Start"
        android:saveEnabled="false"
        android:clickable="true"
        android:src="@drawable/materialdesignicons_clock_start_green"
        style="@style/Widget.ActionPage"
        android:visibility="visible"
        />
    <!--
    android:foreground="#c0c0c0"
    android:src="@android:drawable/ic_menu_preferences"
    -->



</FrameLayout>